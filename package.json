{
  "name": "clients-service",
  "version": "1.0.0",
  "description": "",
  "main": "main.ts",
  "scripts": {
    "typeorm": "ts-node-dev ./node_modules/typeorm/cli.js",
    "build": "tsc",
    "start": "npm run build && node dist/server/main.js",
    "start:dev": "nodemon",
    "prepare": "husky install",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "npm test -- -c jest-e2e.config.ts --runInBand",
    "test:unit": "npm test -- -c jest-unit.config.ts --coverage",
    "test:unit:watch": "npm run test:watch -- -c jest-unit.config.ts --coverage",
    "typecheck": "tsc --noEmit",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "style:check": "prettier --check 'src/**/*.ts' 'test/**/*.ts'",
    "style:fix": "prettier --write 'src/**/*.ts' 'test/**/*.ts'",
    "migration:run": "npm run build && npm run typeorm -- -d server/database/typeorm/data-source.typeorm.ts migration:run",
    "migration:create": "npm run typeorm migration:create -n server/database/typeorm/migrations/${filename}"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "dotenv": "^16.1.3",
    "express": "^4.18.2",
    "loadsh": "^0.0.4",
    "nodemon": "^2.0.22",
    "pg": "^8.11.0",
    "reflect-metadata": "^0.1.13",
    "ts-jest": "^29.1.0",
    "typeorm": "^0.3.16"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/node": "^20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "concurrently": "^8.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4"
  }
}
